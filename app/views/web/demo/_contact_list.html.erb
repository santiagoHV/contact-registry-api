<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Fecha de nacimiento</th>
      <th>Direcci√≥n</th>
      <th>Casa/apartamento</th>
      <th>Ciudad</th>
      <th>Actualizar</th>
      <th>Borrar</th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr id="contact-<%= contact.id %>">
        <td><%= contact.id %></td>
        <td><%= contact.name %></td>
        <td><%= contact.lastname %></td>
        <td><%= contact.birthdate %></td>
        <td><%= contact.address %></td>
        <td><%= contact.address_detail %></td>
        <td><%= contact.city.name %></td>
        <td>
          <button class="btn btn-primary" onclick="onUpdate(<%= contact.id %>)">Actualizar</button>
        </td>
        <td>
          <button class="btn btn-danger" onclick="onDelete(<%= contact.id %>)">Borrar</button>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>